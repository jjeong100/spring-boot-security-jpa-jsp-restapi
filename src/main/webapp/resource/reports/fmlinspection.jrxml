	<queryString>
		<![CDATA[SELECT 
		tmp2.workstationName,
		tmp2.productName,
		tmp2.productEan,
		tmp2.usage,
		prod.name as rawMaterialName,
		prod.standard as rawMaterialStandard,
		(SELECT array_to_string(array_agg(heatnumber),',') 
				from (
						select heatnumber
						from(
							  select sum(sumquantity) as sumquantity ,orderdec_id,delivery_id
							  from 
							  (select *,
							  case 
							  	when type ='03internalOutbound' then quantity 
							  	when type ='08internalCanbound' then -quantity 
							  end as sumquantity
							  from materialflowresources_position mp
							  where orderdec_id=tmp2.orderId and  to_char(mp.createdate,'yyyy-mm-dd') between (select to_char(now(),'yyyy-mm-dd')) and (SELECT to_char('tomorrow'::TIMESTAMP,'yyyy-mm-dd'))
							  )as aaa
						 where sumquantity!=0  
						  group by orderdec_id, delivery_id
						  )tep
						  inner join deliveries_deliveredproduct ddp on ddp.delivery_id=tep.delivery_id
						  )tcp) as heatnumber,
		  to_char(now(),'yyyy-mm-dd') as createdate,
		  operationId,
		  operationName,
		  (select to_char(starttime,'HH24:MI') as starttime 
		  from technologies_operationinspectiontimestandard tot 
		  where tmp2.technologyprototypeId=tot.technology_id 
		  and tot.operation_id=tmp2.operationId 
		  and tot.status='01Save'
		  and type='01ProductionFirstInspection' LIMIT 1) as pfirststime,
		  (select to_char(endtime,'HH24:MI') as endtime 
		  from technologies_operationinspectiontimestandard tot 
		  where tmp2.technologyprototypeId=tot.technology_id 
		  and tot.operation_id=tmp2.operationId 
		  and tot.status='01Save'
		  and type='01ProductionFirstInspection' LIMIT 1) as pfirstetime,
		  (select to_char(starttime,'HH24:MI') as starttime 
		  from technologies_operationinspectiontimestandard tot 
		  where tmp2.technologyprototypeId=tot.technology_id 
		  and tot.operation_id=tmp2.operationId 
		  and tot.status='01Save'
		  and type='02ProductionMiddleInspection' LIMIT 1) as pmiddlestime,
		  (select to_char(endtime,'HH24:MI') as endtime 
		  from technologies_operationinspectiontimestandard tot 
		  where tmp2.technologyprototypeId=tot.technology_id 
		  and tot.operation_id=tmp2.operationId 
		  and tot.status='01Save'
		  and type='02ProductionMiddleInspection' LIMIT 1) as pmiddleetime,
		  (select to_char(starttime,'HH24:MI') as starttime 
		  from technologies_operationinspectiontimestandard tot 
		  where tmp2.technologyprototypeId=tot.technology_id 
		  and tot.operation_id=tmp2.operationId 
		  and tot.status='01Save'
		  and type='03ProductionLastInspection' LIMIT 1) as plaststime,
		  (select to_char(endtime,'HH24:MI') as endtime 
		  from technologies_operationinspectiontimestandard tot 
		  where tmp2.technologyprototypeId=tot.technology_id 
		  and tot.operation_id=tmp2.operationId 
		  and tot.status='01Save'
		  and type='03ProductionLastInspection' LIMIT 1) as plastetime,
		  (select to_char(starttime,'HH24:MI') as starttime 
		  from technologies_operationinspectiontimestandard tot 
		  where tmp2.technologyprototypeId=tot.technology_id 
		  and tot.operation_id=tmp2.operationId 
		  and tot.status='01Save'
		  and type='04ProductionExtraInspection' LIMIT 1) as pextrastime,
		  (select to_char(endtime,'HH24:MI') as endtime 
		  from technologies_operationinspectiontimestandard tot 
		  where tmp2.technologyprototypeId=tot.technology_id 
		  and tot.operation_id=tmp2.operationId 
		  and tot.status='01Save'
		  and type='04ProductionExtraInspection' LIMIT 1) as pextraetime,
		  (select to_char(starttime,'HH24:MI') as starttime 
		  from technologies_operationinspectiontimestandard tot 
		  where tmp2.technologyprototypeId=tot.technology_id 
		  and tot.operation_id=tmp2.operationId 
		  and tot.status='01Save'
		  and type='05QualityFirstInspection' LIMIT 1) as qfirststime,
		  (select to_char(endtime,'HH24:MI') as endtime 
		  from technologies_operationinspectiontimestandard tot 
		  where tmp2.technologyprototypeId=tot.technology_id 
		  and tot.operation_id=tmp2.operationId 
		  and tot.status='01Save'
		  and type='05QualityFirstInspection' LIMIT 1) as qfirstetime,
		  (select to_char(starttime,'HH24:MI') as starttime 
		  from technologies_operationinspectiontimestandard tot 
		  where tmp2.technologyprototypeId=tot.technology_id 
		  and tot.operation_id=tmp2.operationId 
		  and tot.status='01Save'
		  and type='06QualityMiddleInspection' LIMIT 1) as qmiddlestime,
		  (select to_char(endtime,'HH24:MI') as endtime 
		  from technologies_operationinspectiontimestandard tot 
		  where tmp2.technologyprototypeId=tot.technology_id 
		  and tot.operation_id=tmp2.operationId 
		  and tot.status='01Save'
		  and type='06QualityMiddleInspection' LIMIT 1) as qmiddleetime,
		  (select to_char(starttime,'HH24:MI') as starttime 
		  from technologies_operationinspectiontimestandard tot 
		  where tmp2.technologyprototypeId=tot.technology_id 
		  and tot.operation_id=tmp2.operationId 
		  and tot.status='01Save'
		  and type='07QualityExtraInspection' LIMIT 1) as qextrastime,
		  (select to_char(endtime,'HH24:MI') as endtime 
		  from technologies_operationinspectiontimestandard tot 
		  where tmp2.technologyprototypeId=tot.technology_id 
		  and tot.operation_id=tmp2.operationId 
		  and tot.status='01Save'
		  and type='07QualityExtraInspection' LIMIT 1) as qextraetime
		  
FROM (
		select 
				tmp.order_id as orderId,
			    tmp.workstationid as workstationId,
				oo.name as orderName,
				oo.number as orderNumber,
				oo.lot as productLot,
				bp.name as productName,
				ppl.name as lineName,
				bw.name as workstationName,
		 		bp.usage as usage,
		 		bp.ean as productEan,
				to_char(now(),'yyyy-mm-dd') as createDate,
				oo.technology_id as technologyId,
				oo.technologyprototype_id as technologyprototypeId,
				operationId,
				tlo.name as operationName
		from (
				select 
				count(*),
				order_id,
				workstationid,
				ttoc.operation_id as operationId
					from qualitycontrols_productioninspection qp
					inner join technologies_technologyoperationcomponent ttoc on ttoc.id=qp.technologyoperationcomponent_id
					group by order_id,workstationid,ttoc.operation_id
		) tmp 
		inner join orders_order oo on oo.id=tmp.order_id
		inner join basic_product bp on bp.id=oo.product_id
		inner join productionlines_productionline ppl on ppl.id=oo.productionline_id
		inner join basic_workstation bw on bw.id=tmp.workstationid
		inner join technologies_operation tlo on tlo.id=tmp.operationId
		where tmp.order_id=$P{orderId} and tmp.workstationid=$P{workstationId}
)tmp2				
inner join technologies_technology tt on tmp2.technologyId=tt.id
inner join technologies_technologyoperationcomponent ttoc on tt.id=ttoc.technology_id
INNER JOIN technologies_operationproductincomponent weari ON ttoc.id = weari.operationcomponent_id
INNER JOIN basic_product prod ON weari.product_id = prod.id
WHERE 1=1
AND prod.globaltypeofmaterial = '01component';]]>
	</queryString>
